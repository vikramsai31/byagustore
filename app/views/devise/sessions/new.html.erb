
<div class="span10">
<div class="span9">
	 <hr class="soft"/>
    <ul class="breadcrumb">
		<li><a href="index.html">Home</a> <span class="divider">/</span></li>
		<li class="active">Sign In</li>
    </ul>
    
   

   

<div class="well">
		<h3> I AM ALREADY REGISTERED </h3>
		 
		
			<%= form_for(resource, :as => resource_name, :url => session_path(resource_name),:class=>"form-horizontal") do |f| %>
			
			<div class=""><%= devise_error_messages! %></div>
		<div class="control-group">
				 	<%= f.label :email , :class=>"control-label"%>
				  <div class="controls">
					<%= f.email_field :email, :autofocus => true %>
				  </div>
				</div>
				<div class="control-group">
				 	<%= f.label :password,  :class=>"control-label" %>
				  <div class="controls">
					 <%= f.password_field :password %>
				  </div>
				</div>
				 <% if devise_mapping.rememberable? -%>
  <div class="control-group">
 	<div class="controls">
    <%= f.check_box :remember_me %> <%= f.label :remember_me %></div></div>
  <% end -%>
				<div class="control-group">
				  <div class="controls">
					<%= f.submit "Sign in", :class=> 'btn'%> OR <%= link_to "Register", new_registration_path(resource_name) , :class=> 'btn'%>
				  </div>
				</div>
				
			<% end %>
			
   <%= render "devise/shared/links" %>
					
		<div>


</div>
</div>



